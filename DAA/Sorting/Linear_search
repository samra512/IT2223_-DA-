  % Linear search
      ------------------------------

arr=[3,5,7,9,11,13];
tar=9;
n=length(arr);
found=false;

tic;
for i=1:1:n
 if arr(i)==tar
     found=true;
     break;    
 end
end

 if found
        disp([ 'index of target elemet is:',num2str(i)])
 else
     disp('element is not found')
 end

 elapsedTime=toc;
 fprintf('Elapsed time:%fseconds\n',elapsedTime);

output:
Elapsed time:0.000758seconds

